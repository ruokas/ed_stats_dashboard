{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app.js",
    "theme-init.js",
    "service-worker.js",
    "data-worker.js",
    "scripts/perf-harness.mjs",
    "src/main.js",
    "src/app/runtime.js",
    "src/app/runtime-full.js",
    "src/app/full-page-app.js",
    "src/events/tv.js",
    "src/app/runtime/pages/*.js",
    "src/app/runtime/runtimes/*.js",
    "src/app/runtime/runtimes/summaries/*.js"
  ],
  "project": ["src/**/*.js", "tests/**/*.js", "*.js", "scripts/**/*.mjs"],
  "ignoreIssues": {
    "src/data/stats.js": ["exports"],
    "src/utils/format.js": ["exports"],
    "src/app/runtime/features/theme.js": ["exports"],
    "src/app/runtime/utils/color.js": ["exports"],
    "src/app/runtime/core-context.js": ["exports"],
    "src/app/runtime/chart-primitives.js": ["exports"],
    "src/app/runtime/settings.js": ["exports"]
  },
  "ignore": ["coverage/**", "docs/**", ".husky/**"]
}
